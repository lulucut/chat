extends layout
block content
	div.wrapper
		div.user-list
			ul#users
				li.user-all.active(data-id="group")
					span#peopel 0 
					span user online
					span.count 0
		div#msg-area
			div#group.msg.show(data-id="group")
				p.attention 提示：点击头像或用户名可以进入私聊
		div.btns
			button#image image
			input#file(type="file")
		textarea#msg-input(placeholder="enter to send")
		button#send send
